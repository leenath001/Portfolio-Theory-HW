   "source": [
    "# Covariance matrix\n",
    "cov_mat = data.cov()\n",
    "\n",
    "# Weights \n",
    "w = np.linalg.inv(cov_mat) @ data.mean()\n",
    "w_norm = w / w.sum()\n",
    "\n",
    "res = pd.DataFrame({\"Weight\": w_norm, \"Sharpe\": data_sharpe}).round(4)\n",
    "res"
   ]
  },
